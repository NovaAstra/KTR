{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "@internal/tsconfig-vue",
	"extends": "./web.json",
	"compilerOptions": {
		"exactOptionalPropertyTypes": false,
		"noEmit": true,
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"jsx": "preserve",
		"jsxFactory": "h",
		"jsxImportSource": "vue",
		"declaration": false,
		"declarationMap": false
	},
	"vueCompilerOptions": {
		"target": 3
	}
}
